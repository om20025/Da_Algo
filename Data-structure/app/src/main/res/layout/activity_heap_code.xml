<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Heap_code"
    android:fitsSystemWindows="true"
    android:id="@+id/heap_code"
    tools:openDrawer="start"
    android:background="#FFFFF0">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/heap_code_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/colorPrimary"
            android:backgroundTint="#ffa500"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:title="Heap"
            app:titleMarginStart="40dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="6dp"
            tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/heap_text7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_code_toolbar"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text7"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text8"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text9"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text10"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text11"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text12"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/heap_text14"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:layout_marginRight="10dp"
        android:textAlignment="gravity"
        app:layout_constraintTop_toBottomOf="@+id/heap_text13"
        tools:ignore="MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteX="0dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.drawerlayout.widget.DrawerLayout>