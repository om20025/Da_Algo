<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:id="@+id/Main_page"
    android:fitsSystemWindows="true"
    tools:openDrawer="start"
    android:background="#FFFFF0">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/ToolBar"
                android:backgroundTint="#ffa500"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/colorPrimary"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                tools:ignore="MissingConstraints"
                app:title="Data Structures..."
                app:titleMarginStart="20dp"/>

            <TextView
                android:id="@+id/main_textview1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textAlignment="center"
                android:layout_marginRight="10dp"
                android:textSize="24dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textStyle="bold|italic"
                app:layout_constraintTop_toBottomOf="@+id/ToolBar"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="0dp" />

            <TextView
                android:id="@+id/main_textview2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                android:textSize="14dp"
                android:fontFamily="sans-serif-light"
                app:layout_constraintTop_toBottomOf="@+id/main_textview1"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="0dp"
                />

            <TextView
                android:id="@+id/main_textview3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:textSize="24dp"
                android:textColor="@color/black"
                android:textStyle="bold|italic"
                android:fontFamily="sans-serif-light"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview2"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="0dp"
                />

            <TextView
                android:id="@+id/main_textview4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:fontFamily="sans-serif-light"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview3"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="0dp"
                />

            <TextView
                android:id="@+id/main_textview5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_array"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview4"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_linkedlist_main"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview5"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />


            <TextView
                android:id="@+id/main_textview7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textStyle="bold|italic"
                android:textSize="24dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview6"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview7"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_stack"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview8"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_queue"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview9"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_tree"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview10"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_heap"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview11"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_graph"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview12"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

            <TextView
                android:id="@+id/main_textview14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:textAlignment="gravity"
                android:layout_marginLeft="10dp"
                android:drawableLeft="@drawable/ic_hashtable"
                android:drawablePadding="20dp"
                android:fontFamily="sans-serif-light"
                android:textColor="@color/black"
                android:textSize="14dp"
                app:layout_constraintTop_toBottomOf="@+id/main_textview13"
                tools:ignore="MissingConstraints,UnknownIdInLayout"
                tools:layout_editor_absoluteX="65dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>
    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/Navigation"
        app:headerLayout="@layout/headerfile"
        app:menu="@menu/sidemenu"
        android:layout_gravity="start">

    </com.google.android.material.navigation.NavigationView>




</androidx.drawerlayout.widget.DrawerLayout>